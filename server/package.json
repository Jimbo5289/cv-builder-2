{
  "name": "cv-builder-server",
  "version": "1.0.0",
  "description": "CV Builder Backend Server",
  "main": "src/index.js",
  "scripts": {
    "start": "node debug-ip.js && node src/index.js",
    "dev": "npx nodemon src/index.js",
    "dev:managed": "node start-server.js",
    "test": "jest",
    "migrate": "node prisma/apply-migration.js",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "render": "node render-setup.js",
    "render:start": "node pre-start.js",
    "debug:port": "node debug-port.js",
    "render:debug": "node debug-port.js",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org=${SENTRY_ORG} --project=cv-builder-server ./dist && sentry-cli sourcemaps upload --org=${SENTRY_ORG} --project=cv-builder-server ./dist"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@sentry/cli": "^2.43.1",
    "@sentry/integrations": "^7.91.0",
    "@sentry/node": "^7.91.0",
    "@sentry/profiling-node": "^1.2.6",
    "@sentry/tracing": "^7.120.3",
    "@stripe/stripe-js": "^7.3.1",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cuid": "^3.0.0",
    "dotenv": "^16.4.4",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-ws": "^5.0.2",
    "googleapis": "^148.0.0",
    "helmet": "^7.1.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.9.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.9.9",
    "openai": "^4.28.0",
    "pdfjs-dist": "^5.3.31",
    "pdfkit": "^0.17.0",
    "pdfmake": "^0.2.20",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "stripe": "^14.25.0",
    "winston": "^3.11.0",
    "ws": "^8.18.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^5.22.0",
    "supertest": "^7.1.0"
  }
}
