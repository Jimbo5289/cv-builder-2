{
  "name": "cv-builder",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "dev": "node start-dev.js",
    "dev:simple": "node dev.js",
    "dev:clean": "node start-clean.js",
    "dev:vite": "vite",
    "dev:legacy": "vite",
    "build": "vite build",
    "build:verify": "vite build && node verify-build.cjs",
    "preview": "vite preview",
    "start": "bash scripts/clean-and-start.sh",
    "start:ultimate": "node start-ultimate.cjs",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "analyze": "vite build --mode analyze",
    "server:check": "node check-server.js",
    "server:kill": "lsof -ti:3005-3010,5173-5190 | xargs kill -9 2>/dev/null || true",
    "debug:router": "node scripts/debug-router.js",
    "server:dev": "cd server && npm run dev",
    "frontend:dev": "vite",
    "port:reset": "rm -f .port-config.json && echo 'Port configuration reset'"
  },
  "dependencies": {
    "@sentry/react": "^9.22.0",
    "@stripe/stripe-js": "^7.3.1",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.1",
    "pdfmake": "^0.2.20",
    "Prettier": "npm:prettier@^3.5.3",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.0.1",
    "react-pdf": "^9.2.1",
    "react-router-dom": "^6.11.1",
    "react-select": "^5.7.3"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.23",
    "prettier": "^3.2.2",
    "tailwindcss": "^3.3.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.2"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
