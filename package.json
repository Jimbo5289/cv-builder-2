{
  "name": "cv-builder",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "vite": "vite",
    "build": "vite build",
    "build:verify": "vite build && node verify-build.js",
    "build:vercel": "node --no-warnings build.js",
    "build:vercel:cjs": "node --no-warnings build-cjs.js",
    "vercel-build": "npm install --production=false && npm run build:verify",
    "postbuild": "node verify-build.cjs && npm run copy-debug-files",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org=${SENTRY_ORG} --project=cv-builder ./dist && sentry-cli sourcemaps upload --org=${SENTRY_ORG} --project=cv-builder ./dist",
    "dev:reliable": "./start-clean.sh",
    "dev:mock": "npm run server:mock & npm run dev",
    "server:mock": "cd server && node mock-mode.js",
    "server:restart": "cd server && node restart-server.js",
    "start": "./server-modes.sh",
    "start:dev": "./server-modes.sh dev",
    "start:mock": "./server-modes.sh mock",
    "start:clean": "node start-clean.js",
    "start:complete": "MOCK_SUBSCRIPTION_DATA=true SKIP_AUTH_CHECK=true ./start-server.js",
    "start:all": "MOCK_SUBSCRIPTION_DATA=true SKIP_AUTH_CHECK=true cd server && npm run dev",
    "start:safe": "node start-reliable.js",
    "dev:clean": "node start-clean.js",
    "kill:all": "pkill -9 -f node || true",
    "server:check": "node check-server.js",
    "server:kill": "lsof -ti:3005,3006,3007,3008,3009,5173,5174,5175,5176,5177 | xargs kill -9 2>/dev/null || true",
    "start:cv": "node cv-start.js",
    "safari:fix": "node safari-fix.js",
    "fix:safari": "node safari-fix.js",
    "ultimate": "node ultimate-fix.js",
    "safari": "node safari-fix.js",
    "fix": "node fix-safari.js",
    "fix:all": "node fix-safari.js",
    "fix:simple": "node fix-safari-simpler.js",
    "fix:setup": "node fix-safari-simpler.js",
    "verify-build": "node verify-build.js",
    "copy-debug-files": "cp -f public/debug.js public/debug-bundle.js public/test.html public/fallback.html public/fallback-app.jsx public/fallback-react.html public/preload-react.js public/react-rescue.js public/check-react.js public/fix-component.js public/component-resolver.js public/direct-fix.js public/lazy-component-fix.js dist/ || true"
  },
  "dependencies": {
    "@sentry/react": "^9.22.0",
    "@stripe/stripe-js": "^7.3.0",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.16.4",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.1",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.1",
    "pdfmake": "^0.2.20",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.0.1",
    "react-native": "^0.72.17",
    "react-pdf": "^9.2.1",
    "react-router-dom": "^6.11.1",
    "react-select": "^5.7.3",
    "react-spring": "^9.7.3",
    "react-to-pdf": "^2.0.0",
    "react-to-print": "^2.14.13",
    "save-html-as-image": "^1.7.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.14",
    "chalk": "^4.1.2",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "vite": "^6.3.5"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-konva": "18.2.10"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-spring": "^9.7.3",
    "react-konva": "18.2.10"
  }
}
